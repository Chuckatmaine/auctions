<h2>Sign in if you have already signed up, else select Sign up</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
 <ul class="formList"; style="list-style-type:none">  
    <li><%= f.label :email, "Email" %> <%= f.text_field :email %></li>  
    <li><%= f.label :password %> <%= f.password_field :password %></li>  
    <% if devise_mapping.rememberable? -%>  
    <li><%= f.check_box :remember_me %> <%= f.label :remember_me %></li>  
    <% end %>  
    <li><%= f.submit "Sign in" %></li>  
  </ul>  
<% end %>

<%= render :partial => "devise/shared/links" %>
